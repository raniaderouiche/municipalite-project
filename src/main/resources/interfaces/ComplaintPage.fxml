<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="512.0" prefWidth="897.0" stylesheets="@../assets/css/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fsb.municipalite.controllers.ComplaintPageController">
   <children>
      <VBox prefHeight="521.0" prefWidth="897.0">
         <children>
            <HBox prefHeight="64.0" prefWidth="897.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onClickEventAdd" prefHeight="25.0" prefWidth="79.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Add">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onClickEventRemove" prefHeight="30.0" prefWidth="79.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Remove">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#monStock" prefHeight="30.0" prefWidth="79.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Refresh">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#UpdateComplaint" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Update">
                     <HBox.margin>
                        <Insets left="15.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  <TextField fx:id="searchBox" onAction="#search" prefHeight="34.0" prefWidth="239.0" promptText="Search.." styleClass="searchField">
                     <HBox.margin>
                        <Insets left="15.0" right="10.0" top="10.0" />
                     </HBox.margin>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </TextField>
               </children>
            </HBox>
            <Pane VBox.vgrow="ALWAYS">
               <children>
                  <TableView fx:id="tableView" editable="true" layoutX="18.0" prefHeight="434.0" prefWidth="868.0">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                     <columns>
                        <TableColumn fx:id="Id" maxWidth="2000.0" prefWidth="72.0" text="Id" />
                        <TableColumn fx:id="Version" maxWidth="2000.0" prefWidth="75.0" text="Version" />
                        <TableColumn fx:id="Date" maxWidth="4000.0" prefWidth="216.0" text="Date" />
                        <TableColumn fx:id="Status" maxWidth="3000.0" prefWidth="75.0" text="Status" />
                        <TableColumn fx:id="Name" maxWidth="4000.0" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="subject" maxWidth="10000.0" prefWidth="75.0" text="Subject" />
                     </columns></TableView>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</Pane>
