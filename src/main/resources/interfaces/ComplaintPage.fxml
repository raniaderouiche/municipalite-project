<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="container" stylesheets="@../assets/css/application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fsb.municipalite.controllers.ComplaintPageController">
   <center>
      <VBox>
         <children>
            <HBox minHeight="-Infinity" prefHeight="50.0" prefWidth="1210.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onClickEventAdd" prefHeight="35.0" prefWidth="135.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Add">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#UpdateComplaint" prefHeight="35.0" prefWidth="135.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Update">
                     <HBox.margin>
                        <Insets right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onClickEventRemove" prefHeight="35.0" prefWidth="135.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Remove">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#monStock" prefHeight="35.0" prefWidth="135.0" styleClass="CustomButtons" stylesheets="@../assets/css/application.css" text="Refresh">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Pane prefHeight="48.0" prefWidth="443.0" HBox.hgrow="ALWAYS" />
                  <TextField fx:id="searchBox" prefHeight="37.0" prefWidth="270.0" promptText="  Search.." styleClass="searchField">
                     <HBox.margin>
                        <Insets right="10.0" top="5.0" />
                     </HBox.margin>
                     <padding>
                        <Insets right="10.0" />
                     </padding>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets left="5.0" />
               </padding>
            </HBox>
            <TableView fx:id="tableView" editable="true" prefHeight="543.0" prefWidth="1211.0">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <columns>
                  <TableColumn fx:id="Id" maxWidth="2000.0" prefWidth="72.0" text="Id" />
                  <TableColumn fx:id="Version" maxWidth="2000.0" prefWidth="75.0" text="Version" />
                  <TableColumn fx:id="Date" maxWidth="4000.0" prefWidth="216.0" text="Date" />
                  <TableColumn fx:id="Status" maxWidth="3000.0" prefWidth="75.0" text="Status" />
                  <TableColumn fx:id="Name" maxWidth="4000.0" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="subject" maxWidth="10000.0" prefWidth="75.0" text="Subject" />
               </columns>
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TableView>
            <HBox prefHeight="55.0" prefWidth="1231.0">
               <children>
                  <Pane minHeight="-Infinity" prefHeight="42.0" prefWidth="1017.0" HBox.hgrow="ALWAYS" />
                  <Button contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#goBack" prefHeight="37.0" prefWidth="199.0" styleClass="CustomButtons" text="Back">
                     <HBox.margin>
                        <Insets bottom="10.0" right="20.0" top="5.0" />
                     </HBox.margin>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <top>
      <Label prefHeight="25.0" prefWidth="326.0" styleClass="headers" text="Complaints" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
