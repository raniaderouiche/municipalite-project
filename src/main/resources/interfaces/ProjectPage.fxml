<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<Pane prefHeight="485.0" prefWidth="943.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fsb.municipalite.controllers.ProjectPageController">
   <children>
      <VBox prefHeight="499.0" prefWidth="943.0">
         <children>
            <HBox prefHeight="103.0" prefWidth="943.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#add" text="Add">
                     <HBox.margin>
                        <Insets left="15.0" top="40.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#remove" text="Remove">
                     <HBox.margin>
                        <Insets left="15.0" top="40.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#update" text="Update">
                     <HBox.margin>
                        <Insets left="15.0" top="40.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#refresh" text="Refresh">
                     <HBox.margin>
                        <Insets left="15.0" top="40.0" />
                     </HBox.margin>
                  </Button>
                  <Pane prefHeight="102.0" prefWidth="109.0" />
                  <TextField fx:id="searchBox" onAction="#search" prefHeight="25.0" prefWidth="222.0" promptText="Search">
                     <HBox.margin>
                        <Insets left="300.0" right="15.0" top="40.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <Pane prefHeight="378.0" prefWidth="943.0">
               <children>
                  <TableView fx:id="tableview" layoutY="1.0" prefHeight="378.0" prefWidth="943.0">
                    <columns>
                      <TableColumn fx:id="id" prefWidth="127.0" text="ID" />
                      <TableColumn fx:id="name" prefWidth="146.0" text="Name" />
                        <TableColumn fx:id="startDate" prefWidth="167.0" text="Start Date" />
                        <TableColumn fx:id="dueDate" prefWidth="169.0" text="Due Date" />
                        <TableColumn fx:id="budget" prefWidth="131.0" text="Budget" />
                        <TableColumn fx:id="version" prefWidth="107.0" text="Version" />
                        <TableColumn fx:id="place" prefWidth="85.0" text="Location" />
                    </columns>
                  </TableView>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</Pane>
