                              <ToggleGroup fx:id="status" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="processed" mnemonicParsing="false" prefHeight="56.0" prefWidth="178.0" text="Processed" toggleGroup="$status">
                           <font>
                              <Font size="18.0" />
                           </font>
