            <GridPane layoutY="5.0" prefHeight="490.0" prefWidth="553.0" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="272.0" minWidth="10.0" prefWidth="152.0" />
